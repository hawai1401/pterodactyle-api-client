{
  "name": "pterodactyl-api-client",
  "version": "2.10.2",
  "description": "SDK TypeScript non officiel et entièrement typé pour l’API Pterodactyl.\nCouvre l’ensemble des endpoints client et application avec une architecture modulaire, orientée classes, conçue pour Node.js et les environnements TypeScript stricts.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "license"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs"
    },
    "./client/account": {
      "types": "./dist/client/account/index.d.ts",
      "import": "./dist/client/account/index.js",
      "require": "./dist/client/account/index.cjs"
    },
    "./client/server": {
      "types": "./dist/client/server/index.d.ts",
      "import": "./dist/client/server/index.js",
      "require": "./dist/client/server/index.cjs"
    },
    "./application": {
      "types": "./dist/application/index.d.ts",
      "import": "./dist/application/index.js",
      "require": "./dist/application/index.cjs"
    },
    "./application/user": {
      "types": "./dist/application/user/index.d.ts",
      "import": "./dist/application/user/index.js",
      "require": "./dist/application/user/index.cjs"
    },
    "./application/location": {
      "types": "./dist/application/location/index.d.ts",
      "import": "./dist/application/location/index.js",
      "req,uire": "./dist/application/location/index.cjs"
    },
    "./application/node": {
      "types": "./dist/application/node/index.d.ts",
      "import": "./dist/application/node/index.js",
      "require": "./dist/application/node/index.cjs"
    },
    "./application/server": {
      "types": "./dist/application/server/index.d.ts",
      "import": "./dist/application/server/index.js",
      "require": "./dist/application/server/index.cjs"
    },
    "./application/nest": {
      "types": "./dist/application/nest/index.d.ts",
      "import": "./dist/application/nest/index.js",
      "require": "./dist/application/nest/index.cjs"
    }
  },
  "scripts": {
    "build": "eslint && tsc && node script/postbuild.js",
    "lint": "eslint --fix"
  },
  "keywords": [
    "pterodactyl",
    "pterodactyl-api",
    "pterodactyl-sdk",
    "pterodactyl-panel",
    "pterodactyl-client",
    "pterodactyl-application",
    "panel",
    "api",
    "rest",
    "sdk",
    "typescript",
    "nodejs",
    "typed-api"
  ],
  "repository": {
    "url": "https://github.com/hawai1401/pterodactyl-api-client"
  },
  "author": "hawai1401 <hawai1401@ik.me> (https://hawai1401.fr)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "@types/ws": "^8.18.1",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "jiti": "^2.6.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  }
}
